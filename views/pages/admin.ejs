</head>

<body style="background-color: #B7DAF3;">

    <% include ../partials/nav.ejs %>
        <div style="margin-top:56px;width:100%;"></div>
        <script src="js/navreset.js"></script>

        <h2>Database Results</h2>

        <ul>
            <% resultsAdmin.forEach(function(r) { %>
                <li><%= r.id_cliente %> - <%= r.id_servicio %> - <%= r.entrada %> - <%= r.salida %></li>
            <% }); %>
            <% resultsClientes.forEach(function(r) { %>
                <li><%= r.id_cliente %> - <%= r.nombre_cliente %></li>
            <% }); %>
            <% resultsServicios.forEach(function(r) { %>
                <li><%= r.id_servicio %> - <%= r.nombre_servicio %></li>
            <% }); %>
        </ul>

        <div class="container">
